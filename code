    }
    student(string name,int roll,string branch)
    {
        this->name=name;
        roll_no=roll;
        this ->branch=branch;
    }
    void getdata()
    {
        cin >>name>>roll_no>>branch;
    }
    void setdata()
    {
        
    }
    void print()
    {
        
        
        cout<<"*************************************"<<endl;
        cout<<"name = "<<name<<endl;
        cout<<"roll no = "<<roll_no<<endl;;
        cout<<"branch = "<<branch<<endl;
    }
    
};

int main()
{   
  int n;
  cout<<"enter no. of students"<<endl;
cin>>n;

  student s[70];
 for(int i =0;i<n;i++)
 {
    s[i].getdata();
 }
  
 for(int i =0;i<n;i++)
 {
    s[i].print();
 }
 
 
 
